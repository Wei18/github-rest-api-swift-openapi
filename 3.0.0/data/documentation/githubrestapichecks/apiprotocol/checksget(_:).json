      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Gets a single check run using its ",
              "type" : "text"
            },
            {
              "code" : "id",
              "type" : "codeVoice"
            },
            {
              "text" : ".",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "[!NOTE]",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "The Checks API only looks for pushes in the repository where the check suite or check run were created. Pushes to a branch in a forked repository are not detected and return an empty ",
                  "type" : "text"
                },
                {
                  "code" : "pull_requests",
                  "type" : "codeVoice"
                },
                {
                  "text" : " array.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Note",
          "style" : "note",
          "type" : "aside"
        },
        {
          "inlineContent" : [
            {
              "text" : "OAuth app tokens and personal access tokens (classic) need the ",
              "type" : "text"
            },
            {
              "code" : "repo",
              "type" : "codeVoice"
            },
            {
              "text" : " scope to use this endpoint on a private repository.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "HTTP ",
                  "type" : "text"
                },
                {
                  "code" : "GET \/repos\/{owner}\/{repo}\/check-runs\/{check_run_id}",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Remark",
          "style" : "note",
          "type" : "aside"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Generated from ",
                  "type" : "text"
                },
                {
                  "code" : "#\/paths\/\/repos\/{owner}\/{repo}\/check-runs\/{check_run_id}\/get(checks\/get)",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
