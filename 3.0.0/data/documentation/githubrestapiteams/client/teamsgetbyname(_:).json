    {
      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Gets a team using the team’s ",
              "type" : "text"
            },
            {
              "code" : "slug",
              "type" : "codeVoice"
            },
            {
              "text" : ". To create the ",
              "type" : "text"
            },
            {
              "code" : "slug",
              "type" : "codeVoice"
            },
            {
              "text" : ", GitHub replaces special characters in the ",
              "type" : "text"
            },
            {
              "code" : "name",
              "type" : "codeVoice"
            },
            {
              "text" : " string, changes all words to lowercase, and replaces spaces with a ",
              "type" : "text"
            },
            {
              "code" : "-",
              "type" : "codeVoice"
            },
            {
              "text" : " separator. For example, ",
              "type" : "text"
            },
            {
              "code" : "\"My TEam Näme\"",
              "type" : "codeVoice"
            },
            {
              "text" : " would become ",
              "type" : "text"
            },
            {
              "code" : "my-team-name",
              "type" : "codeVoice"
            },
            {
              "text" : ".",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "[!NOTE]",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "You can also specify a team by ",
                  "type" : "text"
                },
                {
                  "code" : "org_id",
                  "type" : "codeVoice"
                },
                {
                  "text" : " and ",
                  "type" : "text"
                },
                {
                  "code" : "team_id",
                  "type" : "codeVoice"
                },
                {
                  "text" : " using the route ",
                  "type" : "text"
                },
                {
                  "code" : "GET \/organizations\/{org_id}\/team\/{team_id}",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Note",
          "style" : "note",
          "type" : "aside"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "HTTP ",
                  "type" : "text"
                },
                {
                  "code" : "GET \/orgs\/{org}\/teams\/{team_slug}",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Remark",
          "style" : "note",
          "type" : "aside"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Generated from ",
                  "type" : "text"
                },
                {
                  "code" : "#\/paths\/\/orgs\/{org}\/teams\/{team_slug}\/get(teams\/get-by-name)",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
