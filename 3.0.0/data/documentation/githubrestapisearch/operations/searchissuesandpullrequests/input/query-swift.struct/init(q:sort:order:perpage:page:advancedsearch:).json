                  "type" : "codeVoice"
                },
                {
                  "text" : ", ",
                  "type" : "text"
                },
                {
                  "code" : "reactions-thinking_face",
                  "type" : "codeVoice"
                },
                {
                  "text" : ", ",
                  "type" : "text"
                },
                {
                  "code" : "reactions-heart",
                  "type" : "codeVoice"
                },
                {
                  "text" : ", ",
