        {
          "inlineContent" : [
            {
              "text" : "Marks a thread as “read.” Marking a thread as “read” is equivalent to clicking a notification in your notification inbox on GitHub: https:\/\/github.com\/notifications.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "HTTP ",
                  "type" : "text"
                },
                {
                  "code" : "PATCH \/notifications\/threads\/{thread_id}",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Remark",
          "style" : "note",
          "type" : "aside"
        },
