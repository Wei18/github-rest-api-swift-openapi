          "inlineContent" : [
            {
              "text" : "Marks a thread as “read.” Marking a thread as “read” is equivalent to clicking a notification in your notification inbox on GitHub: https:\/\/github.com\/notifications.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
