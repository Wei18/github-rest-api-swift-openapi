        {
          "inlineContent" : [
            {
              "text" : "Updates the webhook configuration for a repository. To update more information about the webhook, including the ",
              "type" : "text"
            },
            {
              "code" : "active",
              "type" : "codeVoice"
            },
            {
              "text" : " state and ",
              "type" : "text"
            },
            {
              "code" : "events",
              "type" : "codeVoice"
            },
            {
              "text" : ", use “",
              "type" : "text"
            },
            {
              "identifier" : "\/rest\/webhooks\/repos#update-a-repository-webhook",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : ".”",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "OAuth app tokens and personal access tokens (classic) need the ",
              "type" : "text"
            },
            {
              "code" : "write:repo_hook",
              "type" : "codeVoice"
            },
            {
              "text" : " or ",
              "type" : "text"
            },
            {
              "code" : "repo",
              "type" : "codeVoice"
            },
            {
              "text" : " scope to use this endpoint.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
