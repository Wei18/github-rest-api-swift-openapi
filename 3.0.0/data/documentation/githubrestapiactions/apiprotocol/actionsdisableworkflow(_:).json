        {
          "inlineContent" : [
            {
              "text" : "Disables a workflow and sets the ",
              "type" : "text"
            },
            {
              "code" : "state",
              "type" : "codeVoice"
            },
            {
              "text" : " of the workflow to ",
              "type" : "text"
            },
            {
              "code" : "disabled_manually",
              "type" : "codeVoice"
            },
            {
              "text" : ". You can replace ",
              "type" : "text"
            },
            {
              "code" : "workflow_id",
              "type" : "codeVoice"
            },
            {
              "text" : " with the workflow file name. For example, you could use ",
              "type" : "text"
            },
            {
              "code" : "main.yaml",
              "type" : "codeVoice"
            },
            {
              "text" : ".",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "OAuth tokens and personal access tokens (classic) need the ",
              "type" : "text"
            },
            {
              "code" : "repo",
              "type" : "codeVoice"
            },
            {
              "text" : " scope to use this endpoint.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "HTTP ",
                  "type" : "text"
                },
                {
                  "code" : "PUT \/repos\/{owner}\/{repo}\/actions\/workflows\/{workflow_id}\/disable",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
