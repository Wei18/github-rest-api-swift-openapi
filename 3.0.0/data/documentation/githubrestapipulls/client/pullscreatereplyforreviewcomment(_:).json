              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "and “",
              "type" : "text"
            },
            {
              "identifier" : "https:\/\/docs.github.com\/rest\/guides\/best-practices-for-using-the-rest-api",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : ".”",
              "type" : "text"
            }
