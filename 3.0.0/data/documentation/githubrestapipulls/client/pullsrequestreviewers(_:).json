            {
              "text" : "Requests reviews for a pull request from a given set of users and\/or teams.",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "This endpoint triggers ",
              "type" : "text"
