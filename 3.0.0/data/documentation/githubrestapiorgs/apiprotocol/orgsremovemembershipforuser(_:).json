        {
          "inlineContent" : [
            {
              "text" : "In order to remove a userâ€™s membership with an organization, the authenticated user must be an organization owner.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "If the specified user is an active member of the organization, this will remove them from the organization. If the specified user has been invited to the organization, this will cancel their invitation. The specified user will receive an email notification in both cases.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
