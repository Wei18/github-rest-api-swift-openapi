    {
      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Create new or update existing custom property values for repositories in a batch that belong to an organization.",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "Each target repository will have its custom property values updated to match the values provided in the request.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "A maximum of 30 repositories can be updated in a single request.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Using a value of ",
              "type" : "text"
            },
            {
              "code" : "null",
              "type" : "codeVoice"
            },
            {
              "text" : " for a custom property will remove or ‘unset’ the property value from the repository.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "To use this endpoint, the authenticated user must be one of:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "An administrator for the organization.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "A user, or a user on a team, with the fine-grained permission of ",
                      "type" : "text"
                    },
                    {
                      "code" : "custom_properties_org_values_editor",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " in the organization.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "HTTP ",
                  "type" : "text"
                },
                {
                  "code" : "PATCH \/orgs\/{org}\/properties\/values",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Remark",
          "style" : "note",
          "type" : "aside"
        },
