      "content" : [
        {
          "anchor" : "overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Codespaces for the specified users will be billed to the organization.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "To use this endpoint, the access settings for the organization must be set to ",
              "type" : "text"
            },
            {
              "code" : "selected_members",
              "type" : "codeVoice"
            },
            {
              "text" : ".",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "For information on how to change this setting, see “",
              "type" : "text"
            },
            {
              "identifier" : "https:\/\/docs.github.com\/rest\/codespaces\/organizations#manage-access-control-for-organization-codespaces",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : ".”",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "OAuth app tokens and personal access tokens (classic) need the ",
              "type" : "text"
            },
            {
              "code" : "admin:org",
              "type" : "codeVoice"
            },
            {
              "text" : " scope to use this endpoint.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "HTTP ",
                  "type" : "text"
                },
                {
                  "code" : "POST \/orgs\/{org}\/codespaces\/access\/selected_users",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Remark",
          "style" : "note",
          "type" : "aside"
        },
        {
