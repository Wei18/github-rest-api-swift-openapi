        {
          "inlineContent" : [
            {
              "text" : "Checks if a user has permission to be assigned to a specific issue.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "If the ",
              "type" : "text"
            },
            {
              "code" : "assignee",
              "type" : "codeVoice"
            },
            {
              "text" : " can be assigned to this issue, a ",
              "type" : "text"
            },
            {
              "code" : "204",
              "type" : "codeVoice"
            },
            {
              "text" : " status code with no content is returned.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Otherwise a ",
              "type" : "text"
            },
            {
              "code" : "404",
              "type" : "codeVoice"
            },
            {
              "text" : " status code is returned.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
