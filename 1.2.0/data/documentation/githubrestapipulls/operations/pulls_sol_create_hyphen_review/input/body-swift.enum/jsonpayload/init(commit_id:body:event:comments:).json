      "parameters" : [
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The SHA of the commit that needs a review. Not using the latest commit SHA may render your review comment outdated if a subsequent commit modifies the line you specify as the ",
                  "type" : "text"
                },
                {
                  "code" : "position",
                  "type" : "codeVoice"
                },
                {
                  "text" : ". Defaults to the most recent commit in the pull request when you do not specify a value.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "commit_id"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "",
                  "type" : "text"
                },
                {
                  "inlineContent" : [
                    {
                      "text" : "Required",
                      "type" : "text"
                    }
                  ],
                  "type" : "strong"
                },
                {
                  "text" : " when using ",
                  "type" : "text"
                },
                {
                  "code" : "REQUEST_CHANGES",
                  "type" : "codeVoice"
                },
                {
                  "text" : " or ",
                  "type" : "text"
                },
                {
                  "code" : "COMMENT",
                  "type" : "codeVoice"
                },
                {
                  "text" : " for the ",
                  "type" : "text"
                },
                {
                  "code" : "event",
                  "type" : "codeVoice"
                },
                {
                  "text" : " parameter. The body text of the pull request review.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "body"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The review action you want to perform. The review actions include: ",
                  "type" : "text"
                },
                {
                  "code" : "APPROVE",
                  "type" : "codeVoice"
                },
                {
                  "text" : ", ",
                  "type" : "text"
                },
                {
                  "code" : "REQUEST_CHANGES",
                  "type" : "codeVoice"
                },
                {
                  "text" : ", or ",
                  "type" : "text"
                },
                {
                  "code" : "COMMENT",
                  "type" : "codeVoice"
                },
                {
                  "text" : ". By leaving this blank, you set the review action state to ",
                  "type" : "text"
                },
                {
                  "code" : "PENDING",
                  "type" : "codeVoice"
                },
                {
                  "text" : ", which means you will need to ",
                  "type" : "text"
                },
                {
                  "identifier" : "https:\/\/docs.github.com\/rest\/pulls\/reviews#submit-a-review-for-a-pull-request",
