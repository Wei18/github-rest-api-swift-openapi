    {
      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Creates new or updates existing custom properties defined for an organization in a batch.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "If the property already exists, the existing property will be replaced with the new values.",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "Missing optional values will fall back to default values, previous values will be overwritten.",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "E.g. if a property exists with ",
              "type" : "text"
            },
            {
              "code" : "values_editable_by: org_and_repo_actors",
              "type" : "codeVoice"
            },
            {
              "text" : " and it’s updated without specifying ",
              "type" : "text"
            },
            {
              "code" : "values_editable_by",
              "type" : "codeVoice"
            },
            {
              "text" : ", it will be updated to default value ",
              "type" : "text"
            },
            {
              "code" : "org_actors",
              "type" : "codeVoice"
            },
            {
              "text" : ".",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "To use this endpoint, the authenticated user must be one of:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "An administrator for the organization.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "A user, or a user on a team, with the fine-grained permission of ",
                      "type" : "text"
                    },
                    {
                      "code" : "custom_properties_org_definitions_manager",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " in the organization.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "HTTP ",
                  "type" : "text"
                },
                {
                  "code" : "PATCH \/orgs\/{org}\/properties\/schema",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Remark",
          "style" : "note",
          "type" : "aside"
        },
        {
