        {
          "inlineContent" : [
            {
              "text" : "Checks if a user has permission to be assigned to an issue in this repository.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "If the ",
              "type" : "text"
            },
            {
              "code" : "assignee",
              "type" : "codeVoice"
            },
            {
              "text" : " can be assigned to issues in the repository, a ",
              "type" : "text"
            },
            {
              "code" : "204",
              "type" : "codeVoice"
            },
            {
              "text" : " header with no content is returned.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Otherwise a ",
              "type" : "text"
            },
            {
              "code" : "404",
              "type" : "codeVoice"
            },
            {
              "text" : " status code is returned.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
